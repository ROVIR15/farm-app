<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/content_iv_add_livestock"
        android:layout_width="0dp"
        android:layout_height="120dp"
        android:layout_marginEnd="20dp"
        android:background="@drawable/bg_blue_shape_home"
        android:elevation="0dp"
        android:focusable="true"
        android:scaleType="centerCrop"
        app:layout_constraintEnd_toStartOf="@+id/content_iv_display_livestock"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/content_icon_add_livestock"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="16dp"
        android:layout_marginTop="12dp"
        android:elevation="2dp"
        android:src="@drawable/ic_notifications_black_24dp"
        app:layout_constraintStart_toStartOf="@+id/content_iv_add_livestock"
        app:layout_constraintTop_toTopOf="@+id/content_iv_add_livestock" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/content_tv_title_add_livestock"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:elevation="2dp"
        android:fontFamily="@font/roboto_medium"
        android:gravity="center|fill_vertical"
        android:text="@string/tambah_nlivestocks"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@+id/content_iv_add_livestock"
        app:layout_constraintEnd_toEndOf="@+id/content_iv_add_livestock"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/content_icon_add_livestock"
        app:layout_constraintVertical_bias="0.0" />

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/content_iv_display_livestock"
        android:layout_width="0dp"
        android:layout_height="120dp"
        android:scaleType="centerCrop"
        android:src="@drawable/bg_green_shape_home"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/content_iv_add_livestock"
        app:layout_constraintTop_toTopOf="@+id/content_iv_add_livestock" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/content_tv_title_display_livestock"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:elevation="2dp"
        android:fontFamily="@font/roboto_medium"
        android:gravity="center"
        android:text="@string/lihat_nlivestock"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@+id/content_iv_display_livestock"
        app:layout_constraintEnd_toEndOf="@+id/content_iv_display_livestock"
        app:layout_constraintStart_toStartOf="@+id/content_iv_display_livestock"
        app:layout_constraintTop_toBottomOf="@+id/content_tv_workshop" />

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/content_tv_workshop"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="16dp"
        android:layout_marginTop="12dp"
        android:elevation="2dp"
        android:src="@drawable/outline_settings_24"
        app:layout_constraintStart_toStartOf="@+id/content_iv_display_livestock"
        app:layout_constraintTop_toTopOf="@+id/content_iv_display_livestock" />

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/content_iv_daftar_area_ternak"
        android:layout_width="0dp"
        android:layout_height="120dp"
        android:layout_marginTop="12dp"
        android:elevation="0dp"
        android:scaleType="centerCrop"
        android:src="@drawable/bg_blue_shape_home"
        app:layout_constraintEnd_toEndOf="@+id/content_iv_add_livestock"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="@+id/content_iv_add_livestock"
        app:layout_constraintTop_toBottomOf="@+id/content_iv_add_livestock" />

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/content_tv_daftar_area_ternak"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="16dp"
        android:layout_marginTop="12dp"
        android:elevation="2dp"
        android:src="@drawable/ic_notifications_black_24dp"
        app:layout_constraintStart_toStartOf="@+id/content_iv_add_livestock"
        app:layout_constraintTop_toTopOf="@+id/content_iv_daftar_area_ternak" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/content_tv_title_daftar_area_ternak"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:elevation="2dp"
        android:fontFamily="@font/roboto_medium"
        android:gravity="center|fill_vertical"
        android:text="@string/daftar_narea_ternak"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@+id/content_iv_daftar_area_ternak"
        app:layout_constraintEnd_toEndOf="@+id/content_iv_daftar_area_ternak"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/content_iv_daftar_area_ternak"
        app:layout_constraintTop_toBottomOf="@+id/content_tv_daftar_area_ternak"
        app:layout_constraintVertical_bias="0.0" />

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/content_iv_keuangan"
        android:layout_width="0dp"
        android:layout_height="120dp"
        android:layout_marginTop="12dp"
        android:scaleType="centerCrop"
        android:src="@drawable/bg_green_shape_home"
        app:layout_constraintEnd_toEndOf="@+id/content_iv_display_livestock"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/content_iv_display_livestock"
        app:layout_constraintTop_toBottomOf="@+id/content_iv_add_livestock" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/content_tv_title_keuangan"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:elevation="2dp"
        android:fontFamily="@font/roboto_medium"
        android:gravity="center"
        android:text="@string/anggaran_dan_realisasi_nkeuangan"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@+id/content_iv_keuangan"
        app:layout_constraintEnd_toEndOf="@+id/content_iv_keuangan"
        app:layout_constraintStart_toStartOf="@+id/content_iv_keuangan"
        app:layout_constraintTop_toBottomOf="@+id/content_tv_keuangan" />

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/content_tv_keuangan"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="16dp"
        android:layout_marginTop="12dp"
        android:elevation="2dp"
        android:src="@drawable/outline_settings_24"
        app:layout_constraintStart_toStartOf="@+id/content_iv_keuangan"
        app:layout_constraintTop_toTopOf="@+id/content_iv_keuangan" />

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/content_iv_question_setting"
        android:layout_width="0dp"
        android:layout_height="120dp"
        android:layout_marginTop="12dp"
        android:elevation="0dp"
        android:scaleType="centerCrop"
        android:src="@drawable/bg_blue_shape_home"
        app:layout_constraintEnd_toEndOf="@+id/content_iv_daftar_area_ternak"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="@+id/content_iv_daftar_area_ternak"
        app:layout_constraintTop_toBottomOf="@+id/content_iv_daftar_area_ternak" />

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/content_tv_question_setting"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="16dp"
        android:layout_marginTop="12dp"
        android:elevation="2dp"
        android:src="@drawable/ic_notifications_black_24dp"
        app:layout_constraintStart_toStartOf="@+id/content_iv_add_livestock"
        app:layout_constraintTop_toTopOf="@+id/content_iv_question_setting" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/content_tv_title_question_setting"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:elevation="2dp"
        android:fontFamily="@font/roboto_medium"
        android:gravity="center|fill_vertical"
        android:text="@string/question_about_ngetting_started"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@+id/content_iv_question_setting"
        app:layout_constraintEnd_toEndOf="@+id/content_iv_question_setting"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/content_iv_question_setting"
        app:layout_constraintTop_toBottomOf="@+id/content_tv_question_setting"
        app:layout_constraintVertical_bias="0.0" />

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/content_iv_setting"
        android:layout_width="0dp"
        android:layout_height="120dp"
        android:layout_marginTop="12dp"
        android:scaleType="centerCrop"
        android:src="@drawable/bg_green_shape_home"
        app:layout_constraintEnd_toEndOf="@+id/content_iv_keuangan"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/content_iv_keuangan"
        app:layout_constraintTop_toBottomOf="@+id/content_iv_daftar_area_ternak" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/content_tv_settings"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:elevation="2dp"
        android:fontFamily="@font/roboto_medium"
        android:gravity="center"
        android:text="Settings"
        android:textSize="12sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/content_iv_setting"
        app:layout_constraintEnd_toEndOf="@+id/content_iv_setting"
        app:layout_constraintStart_toStartOf="@+id/content_iv_setting"
        app:layout_constraintTop_toBottomOf="@+id/content_tv_setting" />

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/content_tv_setting"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="16dp"
        android:layout_marginTop="12dp"
        android:elevation="2dp"
        android:src="@drawable/outline_settings_24"
        app:layout_constraintStart_toStartOf="@+id/content_iv_setting"
        app:layout_constraintTop_toTopOf="@+id/content_iv_setting" />
</androidx.constraintlayout.widget.ConstraintLayout>