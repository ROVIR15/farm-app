<?xml version="1.0" encoding="utf-8"?>
<androidx.coordinatorlayout.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg_gradient_sign_in"
    tools:context=".ui.daftar_perkawinan.ListAnimalMatingsFragment">

    <include
        android:id="@+id/app_bar_list_animal_matings"
        layout="@layout/app_bar" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/recyclerView_list_animal_matings"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="8dp"
        app:layout_behavior="com.google.android.material.appbar.AppBarLayout$ScrollingViewBehavior"
        tools:listitem="@layout/item_list_animal_matings" />

    <include
        android:id="@+id/loading"
        layout="@layout/loading" />

    <include
        android:id="@+id/data_empty"
        layout="@layout/data_empty" />

</androidx.coordinatorlayout.widget.CoordinatorLayout>